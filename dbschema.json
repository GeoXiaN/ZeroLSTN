{
	"db_name": "ZeroLSTN",
	"db_file": "merged-ZeroLSTN/ZeroLSTN.db",
	"version": 3,
	"maps": {
		".+/data/users/.+/content.json": {
			"to_json_table": ["cert_user_id"],
			"file_name": "data.json"
		},
		".+/data/users/.+/data.json": {
			"to_table": ["songs"]
		}
	},
	"tables": {
		"json": {
		    "cols": [
		        ["json_id", "INTEGER PRIMARY KEY AUTOINCREMENT"],
		        ["site", "TEXT"],
		        ["directory", "TEXT"],
		        ["file_name", "TEXT"],
		        ["cert_user_id", "TEXT"]
		    ],
		    "indexes": ["CREATE UNIQUE INDEX path ON json(directory, site, file_name)"],
		    "schema_changed": 3
		},
		"songs": {
			"cols": [
				["id", "TEXT"],
				["filename", "TEXT"],
			    ["title", "TEXT"],
				["artist", "TEXT"],
				["album", "TEXT"],
				["uploader", "TEXT"],
			    ["date_added", "INTEGER"],
			    ["json_id", "INTEGER REFERENCES json (json_id)"]
			],
			"indexes": ["CREATE UNIQUE INDEX song_key ON songs(json_id, id)", "CREATE INDEX song_date_added_idx ON songs (date_added DESC)"],
			"schema_changed": 2
		}
	}
}